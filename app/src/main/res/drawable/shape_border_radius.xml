<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape android:shape="rectangle" >
            <corners android:radius="25dp" />
            <gradient
                android:angle="90"
                android:startColor="#f4ae12"
                android:endColor="#f1c931"
                android:type="linear" />
        </shape>
    </item>

    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape android:shape="rectangle" >
            <corners android:radius="25dp" />
            <solid android:color="#fff" />
        </shape>
    </item>

</layer-list>