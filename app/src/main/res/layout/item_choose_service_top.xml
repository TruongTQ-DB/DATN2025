<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/cl_container"
    android:layout_width="170dp"
    android:layout_height="@dimen/height_50_dp"
    android:layout_marginEnd="@dimen/margin_16_dp"
    android:layout_marginBottom="@dimen/margin_10_dp"
    android:clickable="true"
    android:elevation="@dimen/margin_10_dp"
    android:focusable="true">

    <TextView
        android:id="@+id/name"
        style="@style/roboto_bold_14dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ellipsize="end"
        android:maxLines="2"
        android:textAlignment="center"
        android:textColor="@color/md_black_1000"
        android:textSize="@dimen/margin_14_dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="30 shine ssssssssbbbbbbbbbbbbbbbbbbbbbbbbbbb" />


</androidx.constraintlayout.widget.ConstraintLayout>